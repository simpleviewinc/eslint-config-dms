		"eslint-plugin-react": "7.37.1",
		"eslint-plugin-react-hooks": "5.1.0-beta-26f2496093-20240514",
		"globals": "15.11.0"
	},
	"peerDependencies": {
		"eslint": ">=9"
	},
	"devDependencies": {
		"@tsconfig/node20": "20.1.4",
		"@types/eslint": "^8.4.5",
		"@types/mocha": "*",
		"@types/node": "18.7.6",
		"@types/react": "*",
