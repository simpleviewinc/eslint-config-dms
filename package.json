{
	"name": "@simpleview/eslint-config-dms",
	"version": "2.0.0",
	"description": "DMS eslint config",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/simpleviewinc/eslint-config-dms.git"
	},
	"author": "Owen Allen <owenallenaz@gmail.com>",
	"license": "ISC",
